<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="CENTER" prefHeight="276.0" prefWidth="442.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="com.example.programmingtest.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Text  strokeType="OUTSIDE" strokeWidth="0.0" text="Username:" />
   <TextField fx:id="Username" />
   <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Password:" />
   <PasswordField fx:id="Password" />

    <Label fx:id="welcomeText" />
    <Button onAction="#onHelloButtonClick" text="login" />
</VBox>
